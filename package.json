{
  "name": "trivy-exporter",
  "version": "1.6.1",
  "type": "module",
  "scripts": {
    "dev": "CRON_RULE=\"0 * * * * *\" node --watch index.ts",
    "lint": "eslint .",
    "check-types": "tsc",
    "prepare": "husky || true",
    "test": "NODE_ENV=test ENABLE_VULN_ID=true node --disable-warning=ExperimentalWarning --test-force-exit --test-concurrency=1 --test test/*.ts",
    "quality": "npm run lint && npm run check-types && npm run test && npm audit --omit=dev --audit-level=critical"
  },
  "license": "MIT",
  "dependencies": {
    "@swimlane/docker-reference": "^2.0.1",
    "node-cron": "^3.0.2",
    "prom-client": "^14.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@types/node": "^24.10.1",
    "@types/node-cron": "^3.0.11",
    "eslint": "^9.39.1",
    "husky": "^9.1.7",
    "neostandard": "^0.12.2",
    "typescript": "^5.9.3"
  }
}
